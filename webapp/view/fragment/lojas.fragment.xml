<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>lojas}"
		class="sapUiPopupWithPadding"
		items="{/CDSet}"
		liveChange="_handleF4LojasSearch"
		search="_handleF4LojasSearch"
		confirm="_handleF4LojasClose"
		cancel="_handleF4LojasCancel"
		multiSelect="true">
		<StandardListItem
			icon=""
			iconDensityAware="false"
			iconInset="false"
			title="{Werks}"
			description="{Nome}"
			info="{UF}"/>
	</SelectDialog>
</core:FragmentDefinition>